{
  "name": "Concept Art",
  "description": "Concept art of various subjects and settings with artistic styles and moods",
  "model_type": "sd",
  "model_settings": {},
  "generation_settings": {
    "sampler": "DPM++ 2M Karras",
    "steps": 30,
    "width": 768,
    "height": 512,
    "batch_size": 1,
    "num_batches": 10,
    "seed": "random"
  },
  "prompt_settings": {
    "base_prompt": "concept art of __SUBJECT__ in __SETTING__, __ARTISTIC__, __MOOD__, __COMPOSITION__",
    "negative_prompt": "low quality, blurry, distorted, unfinished"
  },
  "output_settings": {
    "dir": "./outputs/concept_art/{timestamp}/",
    "format": "png",
    "save_metadata": true,
    "save_prompts": true
  },
  "wildcards": {},
  "wildcard_settings": {},
  "alwayson_scripts": {},
  "forge_api": {
    "base_url": "http://127.0.0.1:7860",
    "timeout": 300,
    "retry_attempts": 3
  }
}